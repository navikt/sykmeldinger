{
    "name": "sykmeldinger",
    "version": "2.0.0",
    "private": true,
    "engines": {
        "node": "18"
    },
    "packageManager": "yarn@3.4.1",
    "scripts": {
        "start": "concurrently -n next,codegen \"yarn start:watch\" \"yarn gen:watch\"",
        "start:watch": "next dev | pino-pretty",
        "start:prod": "NODE_OPTIONS='-r next-logger' next start",
        "gen": "graphql-codegen",
        "gen:watch": "graphql-codegen -w",
        "build": "yarn gen && next build",
        "test": "jest",
        "lint": "next lint",
        "format": "prettier --write 'src/**/**.{ts,tsx,js,jsx}'",
        "report-debt": "scripts/debt.sh",
        "postinstall": "node ./scripts/postinstall.js"
    },
    "dependencies": {
        "@amplitude/analytics-browser": "^1.8.0",
        "@apollo/client": "^3.7.9",
        "@apollo/server": "^4.4.0",
        "@as-integrations/next": "^1.2.0",
        "@graphql-tools/graphql-file-loader": "^7.5.16",
        "@graphql-tools/load": "^7.8.12",
        "@graphql-tools/schema": "^9.0.16",
        "@graphql-typed-document-node/core": "^3.1.2",
        "@navikt/ds-css": "2.4.0",
        "@navikt/ds-icons": "2.4.0",
        "@navikt/ds-react": "2.4.0",
        "@navikt/nav-dekoratoren-moduler": "^1.9.0",
        "@navikt/next-auth-wonderwall": "^1.2.0",
        "@navikt/next-logger": "^1.14.0",
        "@next/bundle-analyzer": "^13.2.1",
        "@react-pdf/renderer": "^3.1.6",
        "classnames": "^2.3.2",
        "crypto-hash": "^2.0.1",
        "date-fns": "^2.29.3",
        "graphql": "^16.6.0",
        "graphql-scalars": "^1.20.1",
        "next": "13.2.1",
        "next-logger": "^3.0.1",
        "pino": "^8.11.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-hook-form": "7.43.2",
        "remeda": "^1.8.0",
        "uuid": "^9.0.0",
        "zod": "^3.20.6"
    },
    "devDependencies": {
        "@graphql-codegen/add": "4.0.1",
        "@graphql-codegen/cli": "3.2.1",
        "@graphql-codegen/fragment-matcher": "^4.0.1",
        "@graphql-codegen/typed-document-node": "3.0.1",
        "@graphql-codegen/typescript-operations": "3.0.1",
        "@graphql-codegen/typescript-resolvers": "3.1.0",
        "@hookform/devtools": "^4.3.0",
        "@navikt/ds-tailwind": "^2.6.0",
        "@navikt/eslint-config-teamsykmelding": "^1.4.0",
        "@testing-library/jest-dom": "^5.16.5",
        "@testing-library/react": "^14.0.0",
        "@testing-library/user-event": "^14.4.3",
        "@types/jest-axe": "^3.5.5",
        "@types/jsdom": "^21.1.0",
        "@types/react": "^18.0.28",
        "@types/react-dom": "^18.0.11",
        "@types/uuid": "^9.0.1",
        "autoprefixer": "^10.4.13",
        "concurrently": "^7.6.0",
        "eslint": "^8.35.0",
        "eslint-config-next": "^13.2.3",
        "husky": "^8.0.3",
        "jest": "^29.4.3",
        "jest-axe": "^7.0.0",
        "jest-environment-jsdom": "^29.4.3",
        "jsdom": "^21.1.0",
        "lint-staged": "^13.1.2",
        "next-router-mock": "^0.9.2",
        "node-gyp": "^9.3.1",
        "open": "^8.4.2",
        "pino-pretty": "^9.3.0",
        "postcss": "^8.4.21",
        "postcss-import": "14.1.0",
        "prettier": "^2.8.4",
        "prettier-plugin-tailwindcss": "^0.2.4",
        "tailwindcss": "^3.2.7",
        "typescript": "^4.9.5"
    },
    "resolutions": {
        "ansi-regex": "5.0.1"
    },
    "prettier": "@navikt/eslint-config-teamsykmelding/prettier",
    "eslintConfig": {
        "extends": [
            "@navikt/eslint-config-teamsykmelding",
            "next/core-web-vitals"
        ],
        "rules": {
            "postcss-modules/no-undef-class": "off",
            "postcss-modules/no-unused-class": "off"
        }
    }
}
