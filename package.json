{
    "name": "sykmeldinger",
    "version": "2.0.0",
    "private": true,
    "packageManager": "yarn@3.2.3",
    "scripts": {
        "start": "concurrently -n next,codegen \"yarn start:watch\" \"yarn gen:watch\"",
        "start:watch": "next dev | pino-pretty",
        "start:prod": "NODE_OPTIONS='-r next-logger' next start",
        "gen": "graphql-codegen",
        "gen:watch": "graphql-codegen -w",
        "build": "yarn gen && next build",
        "test": "jest",
        "lint": "next lint",
        "format": "prettier --write 'src/**/**.{ts,tsx,js,jsx}'",
        "report-debt": "scripts/debt.sh",
        "postinstall": "node ./scripts/postinstall.js"
    },
    "dependencies": {
        "@apollo/client": "^3.6.9",
        "@graphql-tools/graphql-file-loader": "^7.5.5",
        "@graphql-tools/load": "^7.7.7",
        "@graphql-tools/schema": "^8.5.1",
        "@graphql-typed-document-node/core": "^3.1.1",
        "@navikt/ds-css": "^0.18.29",
        "@navikt/ds-datepicker": "^2.1.8",
        "@navikt/ds-icons": "^0.8.20",
        "@navikt/ds-react": "^0.19.27",
        "@navikt/nav-dekoratoren-moduler": "^1.7.0",
        "@navikt/next-logger": "^1.11.0",
        "@next/bundle-analyzer": "^12.3.1",
        "@react-pdf/renderer": "^2.3.0",
        "@sentry/nextjs": "^7.14.0",
        "amplitude-js": "^8.21.1",
        "apollo-server-micro": "^3.10.2",
        "classnames": "^2.3.2",
        "date-fns": "^2.29.3",
        "dayjs": "^1.11.5",
        "graphql": "^16.6.0",
        "graphql-scalars": "^1.18.0",
        "jose": "^4.10.0",
        "micro": "^9.4.1",
        "next": "12.3.1",
        "next-logger": "^3.0.1",
        "node-cache": "^5.1.2",
        "openid-client": "^5.1.10",
        "pino": "^8.7.0",
        "react": "^17.0.2",
        "react-day-picker": "^7.4.10",
        "react-dom": "^17.0.2",
        "react-error-boundary": "^3.1.4",
        "react-hook-form": "^7.36.1",
        "remeda": "^1.1.0",
        "typescript": "^4.8.4",
        "uuid": "^9.0.0",
        "zod": "^3.19.1"
    },
    "devDependencies": {
        "@graphql-codegen/add": "^3.2.1",
        "@graphql-codegen/cli": "^2.13.1",
        "@graphql-codegen/typed-document-node": "^2.3.3",
        "@graphql-codegen/typescript-operations": "^2.5.3",
        "@graphql-codegen/typescript-resolvers": "^2.7.3",
        "@navikt/eslint-config-teamsykmelding": "^0.21.0",
        "@testing-library/jest-dom": "^5.16.5",
        "@testing-library/react": "^12.1.5",
        "@testing-library/react-hooks": "^8.0.1",
        "@testing-library/user-event": "^13.5.0",
        "@types/amplitude-js": "^8.16.2",
        "@types/react": "^17.0.50",
        "@types/uuid": "^8.3.4",
        "concurrently": "^7.4.0",
        "eslint": "^8.22.0",
        "eslint-config-next": "^12.2.5",
        "husky": "^8.0.1",
        "jest": "^27.5.1",
        "lint-staged": "^13.0.3",
        "next-router-mock": "^0.7.4",
        "pino-pretty": "^9.1.1",
        "prettier": "^2.7.1"
    },
    "resolutions": {
        "ansi-regex": "5.0.1",
        "pino": "8.4.2"
    },
    "prettier": "@navikt/eslint-config-teamsykmelding/prettier",
    "eslintConfig": {
        "extends": [
            "@navikt/eslint-config-teamsykmelding",
            "next/core-web-vitals"
        ]
    }
}
