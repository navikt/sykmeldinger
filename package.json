{
    "name": "sykmeldinger",
    "version": "2.0.0",
    "private": true,
    "packageManager": "yarn@3.2.1",
    "scripts": {
        "start": "concurrently -n next,codegen \"yarn start:watch\" \"yarn gen:watch\"",
        "start:watch": "next dev",
        "start:prod": "NODE_OPTIONS='-r next-logger' next start",
        "gen": "graphql-codegen",
        "gen:watch": "graphql-codegen -w",
        "build": "yarn gen && next build",
        "test": "jest",
        "lint": "next lint",
        "format": "prettier --write 'src/**/**.{ts,tsx,js,jsx}'",
        "report-debt": "scripts/debt.sh",
        "postinstall": "husky install && ./scripts/strip-less"
    },
    "dependencies": {
        "@apollo/client": "^3.6.9",
        "@graphql-codegen/add": "^3.1.1",
        "@graphql-codegen/cli": "^2.7.0",
        "@graphql-codegen/typed-document-node": "^2.2.14",
        "@graphql-codegen/typescript-operations": "^2.4.3",
        "@graphql-codegen/typescript-resolvers": "^2.6.7",
        "@graphql-tools/graphql-file-loader": "^7.4.0",
        "@graphql-tools/load": "^7.7.0",
        "@graphql-tools/schema": "^8.5.0",
        "@graphql-typed-document-node/core": "^3.1.1",
        "@navikt/ds-css": "^0.18.26",
        "@navikt/ds-datepicker": "^2.1.8",
        "@navikt/ds-icons": "^0.8.18",
        "@navikt/ds-react": "^0.19.19",
        "@navikt/eslint-config-teamsykmelding": "^0.18.0",
        "@navikt/fnrvalidator": "^1.3.0",
        "@navikt/nav-dekoratoren-moduler": "^1.6.17",
        "@sentry/nextjs": "^7.5.0",
        "amplitude-js": "^8.18.5",
        "apollo-server-micro": "^3.9.0",
        "classnames": "^2.3.1",
        "date-fns": "^2.28.0",
        "dayjs": "^1.11.3",
        "graphql": "^16.5.0",
        "graphql-scalars": "^1.17.0",
        "lodash.throttle": "^4.1.1",
        "micro": "^9.3.4",
        "nav-frontend-core": "^5.1.1",
        "nav-frontend-ekspanderbartpanel": "^3.1.4",
        "nav-frontend-ekspanderbartpanel-style": "^1.1.2",
        "nav-frontend-js-utils": "^1.0.20",
        "nav-frontend-knapper": "^2.1.4",
        "nav-frontend-knapper-style": "^1.1.2",
        "nav-frontend-lenker": "^1.1.2",
        "nav-frontend-lenker-style": "^1.1.1",
        "nav-frontend-typografi": "^3.1.2",
        "nav-frontend-typografi-style": "^1.1.1",
        "nav-frontend-veileder": "^2.1.4",
        "nav-frontend-veileder-style": "^2.1.2",
        "nav-frontend-veilederpanel": "^1.1.4",
        "nav-frontend-veilederpanel-style": "1.1.1",
        "next": "12.2.0",
        "next-logger": "^3.0.1",
        "pino": "^8.1.0",
        "prop-types": "^15.8.1",
        "react": "^17.0.2",
        "react-collapse": "^5.1.1",
        "react-day-picker": "^7.4.10",
        "react-dom": "^17.0.2",
        "react-error-boundary": "^3.1.4",
        "react-hook-form": "^7.33.1",
        "typescript": "^4.7.4",
        "zod": "^3.17.3"
    },
    "devDependencies": {
        "@next/bundle-analyzer": "^12.2.0",
        "@testing-library/jest-dom": "^5.16.4",
        "@testing-library/react": "^12.1.5",
        "@testing-library/react-hooks": "^8.0.1",
        "@testing-library/user-event": "^13.5.0",
        "@types/amplitude-js": "^7.0.1",
        "@types/node": "^17.0.45",
        "@types/react": "^17.0.47",
        "@types/react-collapse": "^5.0.1",
        "concurrently": "^7.2.2",
        "eslint": "^8.19.0",
        "eslint-config-next": "^12.2.0",
        "husky": "^8.0.1",
        "jest": "^27.5.1",
        "lint-staged": "^13.0.3",
        "next-router-mock": "^0.6.10",
        "pino-pretty": "^8.1.0",
        "prettier": "^2.7.1"
    },
    "resolutions": {
        "ansi-regex": "5.0.1"
    },
    "prettier": "@navikt/eslint-config-teamsykmelding/prettier",
    "eslintConfig": {
        "extends": [
            "@navikt/eslint-config-teamsykmelding",
            "next/core-web-vitals"
        ]
    }
}
