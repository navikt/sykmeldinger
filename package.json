        "@grafana/faro-web-tracing": "^1.1.4",
        "@graphql-tools/graphql-file-loader": "^8.0.0",
        "@graphql-tools/load": "^8.0.0",
        "@graphql-tools/schema": "^10.0.0",
        "@graphql-typed-document-node/core": "^3.2.0",
        "@navikt/aksel-icons": "5.4.0",
        "@navikt/ds-css": "5.4.0",
        "@navikt/ds-react": "5.4.0",
        "@navikt/nav-dekoratoren-moduler": "^2.1.3",
        "@navikt/next-auth-wonderwall": "^1.5.0",
        "@navikt/next-logger": "^1.19.0",
        "@next/bundle-analyzer": "^13.4.19",
        "@react-pdf/renderer": "^3.1.12",
        "@unleash/nextjs": "^1.2.0",
        "classnames": "^2.3.2",
        "crypto-hash": "^2.0.1",
        "date-fns": "^2.30.0",
        "globalthis-polyfill": "^1.0.0",
        "graphql": "^16.8.0",
        "graphql-scalars": "^1.22.2",
        "next": "13.4.19",
        "next-logger": "^3.0.2",
        "nextleton": "^0.6.1",
        "pino": "^8.15.1",
        "prom-client": "^14.2.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-hook-form": "7.46.1",
        "remeda": "^1.26.0",
        "tailwind-merge": "^1.14.0",
        "uuid": "^9.0.0",
        "zod": "^3.22.2"
