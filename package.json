{
    "name": "sykmeldinger",
    "version": "2.0.0",
    "private": true,
    "packageManager": "yarn@3.2.3",
    "scripts": {
        "start": "concurrently -n next,codegen \"yarn start:watch\" \"yarn gen:watch\"",
        "start:watch": "next dev | pino-pretty",
        "start:prod": "NODE_OPTIONS='-r next-logger' next start",
        "gen": "graphql-codegen",
        "gen:watch": "graphql-codegen -w",
        "build": "yarn gen && next build",
        "test": "jest",
        "lint": "next lint",
        "format": "prettier --write 'src/**/**.{ts,tsx,js,jsx}'",
        "report-debt": "scripts/debt.sh",
        "postinstall": "node ./scripts/postinstall.js"
    },
    "dependencies": {
        "@apollo/client": "^3.7.1",
        "@graphql-tools/graphql-file-loader": "^7.5.9",
        "@graphql-tools/load": "^7.8.4",
        "@graphql-tools/schema": "^9.0.8",
        "@graphql-typed-document-node/core": "^3.1.1",
        "@navikt/ds-css": "^1.5.3",
        "@navikt/ds-icons": "^1.5.3",
        "@navikt/ds-react": "^1.5.3",
        "@navikt/nav-dekoratoren-moduler": "^1.7.0",
        "@navikt/next-auth-wonderwall": "^0.8.0",
        "@navikt/next-logger": "^1.11.0",
        "@next/bundle-analyzer": "^12.3.2",
        "@react-pdf/renderer": "^2.3.0",
        "@sentry/nextjs": "^7.17.3",
        "amplitude-js": "^8.21.2",
        "apollo-server-micro": "^3.10.4",
        "classnames": "^2.3.2",
        "date-fns": "^2.29.3",
        "dayjs": "^1.11.6",
        "graphql": "^16.6.0",
        "graphql-scalars": "^1.20.0",
        "jose": "^4.10.4",
        "micro": "^9.4.1",
        "next": "12.3.2",
        "next-logger": "^3.0.1",
        "node-cache": "^5.1.2",
        "openid-client": "^5.2.1",
        "pino": "^8.7.0",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-hook-form": "^7.39.1",
        "remeda": "^1.2.0",
        "typescript": "^4.8.4",
        "uuid": "^9.0.0",
        "zod": "^3.19.1"
    },
    "devDependencies": {
        "@graphql-codegen/add": "3.2.1",
        "@graphql-codegen/cli": "2.13.11",
        "@graphql-codegen/typed-document-node": "2.3.6",
        "@graphql-codegen/typescript-operations": "2.5.6",
        "@graphql-codegen/typescript-resolvers": "2.7.6",
        "@navikt/eslint-config-teamsykmelding": "^1.2.0",
        "@testing-library/jest-dom": "^5.16.5",
        "@testing-library/react": "^12.1.5",
        "@testing-library/react-hooks": "^8.0.1",
        "@testing-library/user-event": "^13.5.0",
        "@types/amplitude-js": "^8.16.2",
        "@types/jsdom": "^20.0.0",
        "@types/react": "^17.0.52",
        "@types/react-dom": "^17.0.18",
        "@types/uuid": "^8.3.4",
        "concurrently": "^7.5.0",
        "eslint": "^8.26.0",
        "eslint-config-next": "^12.3.2",
        "husky": "^8.0.1",
        "jest": "^29.2.2",
        "jest-environment-jsdom": "^29.2.2",
        "jsdom": "^20.0.2",
        "lint-staged": "^13.0.3",
        "next-router-mock": "^0.7.4",
        "open": "^8.4.0",
        "pino-pretty": "^9.1.1",
        "prettier": "^2.7.1"
    },
    "resolutions": {
        "ansi-regex": "5.0.1"
    },
    "prettier": "@navikt/eslint-config-teamsykmelding/prettier",
    "eslintConfig": {
        "extends": [
            "@navikt/eslint-config-teamsykmelding",
            "next/core-web-vitals"
        ]
    }
}
