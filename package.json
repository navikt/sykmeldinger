{
    "name": "sykmeldinger",
    "version": "2.0.0",
    "private": true,
    "engines": {
        "node": "18"
    },
    "packageManager": "yarn@3.6.1",
    "scripts": {
        "dev": "yarn start",
        "start": "concurrently -n next,codegen \"yarn start:watch\" \"yarn gen:watch\"",
        "start:watch": "next dev | pino-pretty",
        "start:prod": "NODE_OPTIONS='-r next-logger' next start",
        "gen": "graphql-codegen",
        "gen:watch": "graphql-codegen -w",
        "build": "yarn gen && next build && ./scripts/safari12.sh",
        "test": "vitest --config ./vitest.config.ts",
        "lint": "next lint",
        "format": "prettier --write 'src/**/**.{ts,tsx,js,jsx}'",
        "report-debt": "scripts/debt.sh",
        "postinstall": "node ./scripts/postinstall.js"
    },
    "dependencies": {
        "@apollo/client": "^3.8.3",
        "@apollo/server": "^4.9.3",
        "@as-integrations/next": "^2.0.2",
