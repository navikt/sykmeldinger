{
    "name": "sykmeldinger",
    "version": "2.0.0",
    "private": true,
    "packageManager": "yarn@3.2.3",
    "scripts": {
        "start": "concurrently -n next,codegen \"yarn start:watch\" \"yarn gen:watch\"",
        "start:watch": "next dev | pino-pretty",
        "start:prod": "NODE_OPTIONS='-r next-logger' next start",
        "gen": "graphql-codegen",
        "gen:watch": "graphql-codegen -w",
        "build": "yarn gen && next build",
        "test": "jest",
        "lint": "next lint",
        "format": "prettier --write 'src/**/**.{ts,tsx,js,jsx}'",
        "report-debt": "scripts/debt.sh",
        "postinstall": "node ./scripts/postinstall.js"
    },
    "dependencies": {
        "@amplitude/analytics-browser": "^1.6.8",
        "@apollo/client": "^3.7.3",
        "@apollo/server": "^4.3.0",
        "@as-integrations/next": "^1.2.0",
        "@graphql-tools/graphql-file-loader": "^7.5.14",
        "@graphql-tools/load": "^7.8.9",
        "@graphql-tools/schema": "^9.0.13",
        "@graphql-typed-document-node/core": "^3.1.1",
        "@navikt/ds-css": "2.0.5",
        "@navikt/ds-icons": "2.0.5",
        "@navikt/ds-react": "2.0.5",
        "@navikt/nav-dekoratoren-moduler": "^1.8.1",
        "@navikt/next-auth-wonderwall": "^1.2.0",
        "@navikt/next-logger": "^1.14.0",
        "@next/bundle-analyzer": "^12.3.4",
        "@react-pdf/renderer": "^2.3.0",
        "classnames": "^2.3.2",
        "crypto-hash": "^2.0.1",
        "date-fns": "^2.29.3",
        "graphql": "^16.6.0",
        "graphql-scalars": "^1.20.1",
        "next": "12.3.4",
        "next-logger": "^3.0.1",
        "pino": "^8.8.0",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-hook-form": "^7.42.0",
        "remeda": "^1.3.0",
        "uuid": "^9.0.0",
        "zod": "^3.20.2"
    },
    "devDependencies": {
        "@graphql-codegen/add": "3.2.3",
        "@graphql-codegen/cli": "2.16.3",
        "@graphql-codegen/typed-document-node": "2.3.12",
        "@graphql-codegen/typescript-operations": "2.5.12",
        "@graphql-codegen/typescript-resolvers": "2.7.12",
        "@navikt/eslint-config-teamsykmelding": "^1.4.0",
        "@testing-library/jest-dom": "^5.16.5",
        "@testing-library/react": "^12.1.5",
        "@testing-library/react-hooks": "^8.0.1",
        "@testing-library/user-event": "^13.5.0",
        "@types/jest-axe": "^3.5.5",
        "@types/jsdom": "^20.0.1",
        "@types/react": "^17.0.52",
        "@types/react-dom": "^17.0.18",
        "@types/uuid": "^8.3.4",
        "concurrently": "^7.6.0",
        "eslint": "^8.31.0",
        "eslint-config-next": "^12.3.4",
        "husky": "^8.0.3",
        "jest": "^29.3.1",
        "jest-axe": "^7.0.0",
        "jest-environment-jsdom": "^29.3.1",
        "jsdom": "^20.0.3",
        "lint-staged": "^13.1.0",
        "next-router-mock": "^0.8.0",
        "open": "^8.4.0",
        "pino-pretty": "^9.1.1",
        "prettier": "^2.8.2",
        "typescript": "^4.9.4"
    },
    "resolutions": {
        "ansi-regex": "5.0.1"
    },
    "prettier": "@navikt/eslint-config-teamsykmelding/prettier",
    "eslintConfig": {
        "extends": [
            "@navikt/eslint-config-teamsykmelding",
            "next/core-web-vitals"
        ]
    }
}
