        "lint-staged": "^14.0.1",
        "next-router-mock": "^0.9.9",
        "node-gyp": "^9.4.0",
        "open": "^9.1.0",
        "pino-pretty": "^10.2.0",
        "postcss": "^8.4.29",
        "postcss-import": "^15.1.0",
        "prettier": "^3.0.3",
        "prettier-plugin-tailwindcss": "^0.5.4",
        "tailwindcss": "^3.3.3",
        "typescript": "^5.2.2",
        "vitest": "0.34.3",
        "vitest-axe": "^0.1.0",
        "vitest-dom": "^0.1.0"
    },
    "resolutions": {
