type Brukerinformasjon {
    arbeidsgivere: [Arbeidsgiver!]!
}

type Arbeidsgiver {
    orgnummer: String!
    navn: String!
    aktivtArbeidsforhold: Boolean!
    naermesteLeder: NaermesteLeder
}

type NaermesteLeder {
    navn: String!
}

type UtenforVentetid {
    erUtenforVentetid: Boolean!
    oppfolgingsdato: Date
}
